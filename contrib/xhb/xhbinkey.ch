#define XHB_K_SH_PGDN           433   /* Shift-PgDn  */

#define XHB_K_SH_RETURN         434   /* Shift-Enter */
#define XHB_K_SH_ENTER          434   /* Shift-Enter */

#endif /* XHB_INKEY_CH_ */
