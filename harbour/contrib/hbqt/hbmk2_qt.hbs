   aadd( aLine, "" )

   /* Internal class instantiation. TODO: Rename to __QCLASS */
   aadd( aLine, "HB_FUNC( HB_" + uQtObject + " )" )
   aadd( aLine, "{" )
   aadd( aLine, "   static PHB_ITEM s_oClass = NULL;" )
   aadd( aLine, '   HB_TRACE( HB_TR_DEBUG, ( "HB_' + uQtObject +'" ) );' )
